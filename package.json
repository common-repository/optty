    "@wordpress/scripts": "12.6.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "prettier": "2.5.1"
  },
  "resolutions": {
    "ansi-regex": "5.0.1",
    "glob-parent": "5.1.2",
    "node-notifier": "8.0.1",
    "nth-check": "2.0.1",
    "trim": "0.0.3",
    "markdown-it": "12.3.2"
